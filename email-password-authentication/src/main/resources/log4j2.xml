<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error">

  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd hh:mm:ss.SSS a} | %-5p | %-10.10t | %-25.25c{1} | %m{nolookups} %n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="com.krista.extensions.authentication.email_password_authentication" level="debug"/>
  </Loggers>

</Configuration>
